; Copy this file to `.env` and fill in your Firebase project values.
; Location must be exactly: res://addons/godot-firebase/.env

[firebase/environment_variables]
; Required for all services
apiKey="YOUR_WEB_API_KEY"
authDomain="YOUR_PROJECT.firebaseapp.com"
projectId="YOUR_PROJECT_ID"
appId="YOUR_APP_ID"
measurementId="G-XXXXXXXXXX" ; optional (Analytics)

; Optional depending on features you use
databaseURL="https://YOUR_PROJECT-default-rtdb.firebaseio.com"
storageBucket="YOUR_PROJECT.appspot.com"
messagingSenderId="NNNNNNNNNNN"

; Google OAuth 2.0 (Desktop App) for Google Sign-In
clientId="YOUR_DESKTOP_OAUTH_CLIENT_ID.apps.googleusercontent.com"
clientSecret="YOUR_DESKTOP_OAUTH_CLIENT_SECRET"

; Web (HTML5) OAuth 2.0 client (no secret needed on browser)
webClientId="YOUR_WEB_OAUTH_CLIENT_ID.apps.googleusercontent.com"

; Optional: Dynamic Links / Cloud Functions
domainUriPrefix="https://YOUR_ALIAS.page.link"
functionsGeoZone="us-central1"
cacheLocation="" ; optional local cache directory

[firebase/emulators/ports]
; Leave empty unless using Firebase Emulators
authentication=""
firestore=""
realtimeDatabase=""
functions=""
storage=""
dynamicLinks=""

[firebase/auth_providers]
; Only needed if you also enable these providers
facebook_id=""
facebook_secret=""
github_id=""
github_secret=""
twitter_id=""
twitter_secret=""
